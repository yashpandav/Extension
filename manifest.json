{
  "manifest_version": 3,
  "name": "Smart Tab",
  "version": "1.0",
  "description": "A beautiful and intelligent new tab experience",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "geolocation",
    "topSites"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
